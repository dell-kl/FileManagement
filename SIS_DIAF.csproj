<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>.</DockerfileContext>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="appsettings.Development.json" />
    <Content Remove="wwwroot\js\FormularioUsuario.js" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Transact-SQL\SeedRol.sql" />
    <None Remove="Transact-SQL\SeedSucursales.sql" />
    <None Remove="Transact-SQL\SeedTipoArchivo.sql" />
    <None Remove="Transact-SQL\SeedTipoReg.sql" />
    <None Remove="Transact-SQL\SeedUsuarios.sql" />
    <None Remove="Views\Rol\4nodkzqg.ic5~" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="wwwroot\css\footer.css" />
    <_ContentIncludedByDefault Remove="wwwroot\js\FormularioUsuario.js" />
  </ItemGroup>

  <ItemGroup>
	  <None Include="Transact-SQL\SeedRol.sql" />
