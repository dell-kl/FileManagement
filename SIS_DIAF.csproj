	  <None Include="Transact-SQL\SeedTipoArchivo.sql" />
	  <None Include="Transact-SQL\SeedTipoReg.sql" />
	  <None Include="Transact-SQL\SeedUsuarios.sql" />
  </ItemGroup>
	
	
<ItemGroup>
  <!-- Incluir todos los archivos .sql en la carpeta Scripts/Sql y copiarlos a la carpeta de publicación -->
  <None Update="Transact-SQL\**\*.sql">
    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
  </None>
</ItemGroup>


  <ItemGroup>
    <None Include="wwwroot\js\FormularioUsuario.js" />
  </ItemGroup>

